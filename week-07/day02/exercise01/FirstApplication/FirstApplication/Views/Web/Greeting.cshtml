@model FirstApplication.Models.Greeting
<!DOCTYPE HTML>
<html xmlns:th="http://www.thymeleaf.org">
<head>
    <title>Getting Started: Serving Web Content</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/css/style.css")" />

</head>
<body>
    <p>
        @*Hello, @Model.Content! (This site was loaded @Model.Id times since last server start.)*@
        @foreach (var hello in Model.hellos)
        {         
                @hello;<br>               
        }
    </p>
</body>
</html>